   "source": [
    "dp=[[0]*30 for _ in range(30)]\n",
    "def com(n,r):\n",
    "    if dp[n][r]>0:return dp[n][r]\n",
    "    if n==r or r==0:return 1\n",
    "    dp[n][r]=com(n-1,r-1)+com(n-1,r)\n",
    "    return dp[n][r]\n",
    "for _ in range(int(input())):\n",
    "    N,M=map(int,input().split())\n",
    "    print(com(M,N))"
   ]
