   "source": [
    "c = int(input())\n",
    "for _ in range(c):\n",
    "    tmp = list(map(int,input().split()))\n",
    "    n, score = (tmp[0], tmp[1:])\n",
    "    avg = sum(score)/n\n",
    "    count = sum([1 if i > avg else 0 for i in score ])\n",
    "    print(format(round(count/n*100,3),\".3f\")+\"%\")"
   ]
  }
