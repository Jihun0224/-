    "n=int(input())\n",
    "arr=list(map(int,input().split()))\n",
    "a=0\n",
    "while sum(arr)!=0:\n",
    "    for i in range(n):\n",
    "        if arr[i]%2!=0:\n",
    "            arr[i]-=1\n",
    "            a+=1\n",
    "    if sum(arr)==0:break\n",
    "    a+=1\n",
    "    for i in range(n):arr[i]//=2\n",
    "print(a)"
   ]
  }
