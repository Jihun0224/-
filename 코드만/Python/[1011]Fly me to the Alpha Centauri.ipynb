    "for _ in range(int(input())):\n",
    "    x,y=map(int,input().split())\n",
    "    dis=y-x\n",
    "    n=1\n",
    "    while 1:\n",
    "        if dis<=n*(n+1):\n",
    "            break\n",
    "        n+=1\n",
    "    if dis>n*n:print(n*2)\n",
    "    else:print(n*2-1)"
