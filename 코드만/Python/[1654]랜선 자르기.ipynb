   "source": [
    "import sys\n",
    "input=sys.stdin.readline\n",
    "n,k=map(int,input().split())\n",
    "arr=[int(input()) for _ in range(n)]\n",
    "start=1\n",
    "end=max(arr)\n",
    "\n",
    "while start<=end:\n",
    "    mid=(start+end)//2\n",
    "    cnt=0\n",
    "    for i in arr:cnt+=i//mid\n",
    "    if cnt>=k:start=mid+1\n",
    "    else:end=mid-1\n",
    "print(end)"
   ]
