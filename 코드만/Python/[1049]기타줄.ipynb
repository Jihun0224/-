    "I=lambda:map(int,input().split())\n",
    "N,M=I()\n",
    "p,s=1000,1000\n",
    "for _ in range(M):\n",
    "    i,j=I()\n",
    "    p=min(p,i)\n",
    "    s=min(s,j)\n",
    "t=N//6\n",
    "if p>s*6:print(s*N)\n",
    "else:print(min(p*(t+1),p*t+s*(N%6)))"
   ]
