    "from math import inf\n",
    "input()\n",
    "arr=sorted(list(map(int,input().split())))\n",
    "s=0;e=len(arr)-1\n",
    "a=[0,0];m=inf\n",
    "while s<e:\n",
    "    now=arr[s]+arr[e]\n",
    "    if abs(now)<m:\n",
    "        a[0]=arr[s];a[1]=arr[e]\n",
    "        m=abs(now)\n",
    "    if now>0:e-=1\n",
    "    elif now<0:s+=1\n",
    "    else: break\n",
    "print(*a)"
   ]
  }
