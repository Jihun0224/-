   "source": [
    "def pow(a,b,c):\n",
    "    if b==1:\n",
    "        return a%c\n",
    "    temp = pow(a,b//2,c)\n",
    "    if(b%2==1):\n",
    "        return (temp*temp %c)*a%c\n",
    "    return (temp*temp)%c\n",
    "a,b,c = map(int,input().split())\n",
    "\n",
    "print(pow(a,b,c))"
   ]
  }
