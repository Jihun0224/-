    "import heapq\n",
    "I=lambda:list(map(int,input().split()))\n",
    "input()\n",
    "P=[];a=1\n",
    "for p in I():heapq.heappush(P,-p)\n",
    "for c in I():\n",
    "    tmp=-heapq.heappop(P)\n",
    "    if tmp<c:\n",
    "        a=0\n",
    "        break\n",
    "    heapq.heappush(P,-(tmp-c))\n",
    "print(a)"
   ]
