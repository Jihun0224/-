    "count,n=0,int(input())\n",
    "if n<2:list_=[1]\n",
    "    \n",
    "else:list_=list(range(1,n+1))\n",
    "for i in str(reduce((lambda x,y:x*y),list_))[::-1]:\n",
    "    if i!='0':\n",
    "        print(count)\n",
    "        break\n",
    "    count+=1"
