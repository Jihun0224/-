   "source": [
    "from collections import deque\n",
    "import sys\n",
    "input = sys.stdin.readline\n",
    "deq = deque()\n",
    "for c in range(int(input())):\n",
    "    num = int(input())\n",
    "    if 0 != num:\n",
    "        deq.append(num)\n",
    "    else:\n",
    "        deq.pop()\n",
    "print(sum(deq))"
   ]
